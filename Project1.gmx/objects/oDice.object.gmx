        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>draw_sprite_ext(sprite_index, -1, x, y, image_xscale, image_yscale, image_angle, image_blend, image_alpha);

if (!rolled and oPlayer1.bet &gt; 0)
{
    draw_sprite_ext(sRedArrowDown, 0, x, y-12, 1, 1, 0, c_white, 1);
}</string>
